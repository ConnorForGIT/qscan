{
  "name": "qscan",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "bin": {
    "qscan": "./bin/qscan"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "Apache 2.0",
  "dependencies": {
    "async": "^2.6.1",
    "commander": "^2.18.0",
    "queue": "^4.5.0",
    "wd": "^1.10.3"
  },
  "devDependencies": {
    "ghooks": "^2.0.4",
    "validate-commit-msg": "^2.14.0"
  },
  "config": {
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "test",
        "chore",
        "refactor",
        "opti"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "请输入 message 信息!",
      "helpMessage": "Commit message 格式错误, \n请查看规范: http://wiki.corp.qunar.com/pages/viewpage.action?pageId=159698767"
    }
  }
}
